{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "QB64",
	"patterns": [
		{
			"include": "#preprocessor"
		},
		{
			"include": "#declarations"
		},
		{
			"include": "#control-flow"
		},
		{
			"include": "#scoping"
		},
		{
			"include": "#expressions"
		}
	],
	"repository": {
		"preprocessor": {
			"patterns": [
				{
					"include": "#metacommands"
				},
				{
					"include": "#comments"
				},
				{
					"include": "#line-operations"
				},
				{
					"include": "#options"
				},
				{
					"include": "#affix-typing"
				},
				{
					"include": "#preprocessor-conditions"
				},
				{
					"include": "#preprocessor-text-replacement"
				},
				{
					"include": "#preprocessor-file-directives"
				},
				{
					"include": "#preprocessor-control-directives"
				}
			]
		},
		"comments": {
			"patterns": [
				{
					"include": "#single-quote-comments"
				},
				{
					"include": "#statement-comments"
				},
				{
					"include": "#multiline-comments"
				}
			]
		},
		"single-quote-comments": {
			"name": "comment.line.single-quote.QB64",
			"match": "'.*?$"
		},
		"statement-comments": {
			"name": "comment.line.keyword.QB64",
			"begin": "^(?i)(rem)\\b",
			"beginCaptures": {
				"1": {
					"name": "keyword.other.comment.QB64"
				}
			},
			"end": "\n",
			"patterns": [
				{
					"include": "#metacommands"
				}
			]
		},
		"metacommands": {
			"name": "keyword.other.metacommand.QB64",
			"match": "(?i)(?:(?:'|rem[ \t]+)\\$(?:dynamic|include|lang|static)\\b|\\bonce:)"
		},
		"multiline-comments": {
			"name": "comment.block.QB64",
			"begin": "/'",
			"end": "'/"
		},
		"line-operations": {
			"patterns": [
				{
					"name": "keyword.operator.line.continuation.QB64",
					"match": "[ \t]_[ \t]"
				},
				{
					"name": "keyword.operator.line.separator.QB64",
					"match": ":"
				}
			]
		},
		"preprocessor-conditions": {
			"begin": "(?i)^[ \t]*(#[ \t]*if(?:n?def)?)\\b",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.preprocessor.if.begin.QB64"
				}
			},
			"end": "(?i)^[ \t]*(#[ \t]*endif)\\b",
			"endCaptures": {
				"1": {
					"name": "keyword.control.preprocessor.if.end.QB64"
				}
			},
			"patterns": [
				{
					"match": "(?i)^[ \t]*(#[ \t]*elseif)\\b",
					"captures": {
						"1": {
							"name": "keyword.control.preprocessor.if.elseif.QB64"
						}
					}
				},
				{
					"match": "(?i)^[ \t]*(#[ \t]*else)\\b",
					"captures": {
						"1": {
							"name": "keyword.control.preprocessor.if.else.QB64"
						}
					}
				},
				{
					"name": "keyword.control.preprocessor.QB64",
					"match": "(?i)\\bdefined\\b"
				},
				{
					"include": "$self"
				}
			]
		},
		"preprocessor-text-replacement": {
			"patterns": [
				{
					"include": "#preprocessor-define"
				},
				{
					"include": "#preprocessor-undef"
				},
				{
					"include": "#preprocessor-macro"
				}
			]
		},
		"preprocessor-define": {
			"begin": "(?i)^[ \t]*(#[ \t]*define)\\b",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.preprocessor.text-replacement.define.QB64"
				}
			},
			"end": "(?<!_)[ \t]*\n",
			"patterns": [
				{
					"include": "$self"
				}
			]
		},
		"preprocessor-undef": {
			"begin": "(?i)^[ \t]*(#[ \t]*undef)\\b",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.preprocessor.text-replacement.define.QB64"
				}
			},
			"end": "\n"
		},
		"preprocessor-macro": {
			"begin": "(?i)^[ \t]*(#[ \t]*macro)\\b",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.preprocessor.text-replacement.macro.begin.QB64"
				}
			},
			"end": "(?i)^[ \t]*(#[ \t]*endmacro)\\b",
			"endCaptures": {
				"1": {
					"name": "keyword.control.preprocessor.text-replacement.macro.end.QB64"
				}
			},
			"patterns": [
				{
					"name": "keyword.control.preprocessor.text-replacement.macro.QB64",
					"match": "\\?"
				},
				{
					"include": "$self"
				},
				{
					"include": "#preprocessor-operators"
				}
			]
		},
		"preprocessor-operators": {
			"name": "keyword.operator.preprocessor.QB64",
			"match": "##?"
		},
		"preprocessor-file-directives": {
			"patterns": [
				{
					"include": "#preprocessor-include"
				},
				{
					"include": "#preprocessor-inclib"
				},
				{
					"include": "#preprocessor-libpath"
				}
			]
		},
		"preprocessor-include": {
			"begin": "(?i)^[ \t]*(#[ \t]*include)\\b",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.preprocessor.file-directive.include.QB64"
				}
			},
			"end": "\n",
			"patterns": [
				{
					"name": "keyword.control.preprocessor.QB64",
					"match": "(?i)\\bonce\\b"
				},
				{
					"include": "#string-literals"
				},
				{
					"include": "#comments"
				}
			]
		},
		"preprocessor-inclib": {
			"begin": "(?i)^[ \t]*(#[ \t]*inclib)\\b",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.preprocessor.file-directive.inclib.QB64"
				}
			},
			"end": "\n",
			"patterns": [
				{
					"include": "#string-literals"
				},
				{
					"include": "#comments"
				}
			]
		},
		"preprocessor-libpath": {
			"begin": "(?i)^[ \t]*(#[ \t]*libpath)\\b",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.preprocessor.file-directive.libpath.QB64"
				}
			},
			"end": "\n",
			"patterns": [
				{
					"include": "#string-literals"
				},
				{
					"include": "#comments"
				}
			]
		},
		"preprocessor-control-directives": {
			"patterns": [
				{
					"include": "#preprocessor-pragma"
				},
				{
					"include": "#preprocessor-cmdline"
				},
				{
					"include": "#preprocessor-lang"
				},
				{
					"include": "#preprocessor-print"
				},
				{
					"include": "#preprocessor-error"
				},
				{
					"include": "#preprocessor-assert"
				},
				{
					"include": "#preprocessor-line"
				}
			]
		},
		"preprocessor-pragma": {
			"begin": "(?i)^[ \t]*(#[ \t]*pragma)\\b",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.preprocessor.control-directive.pragma.QB64"
				}
			},
			"end": "\n",
			"patterns": [
				{
					"name": "keyword.control.preprocessor.QB64",
					"match": "(?i)\\b(?:asm|extern|once|pop|push|reserve)\\b"
				},
				{
					"include": "#comments"
				}
			]
		},
		"preprocessor-cmdline": {
			"begin": "(?i)^[ \t]*(#[ \t]*cmdline)\\b",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.preprocessor.control-directive.cmdline.QB64"
				}
			},
			"end": "\n",
			"patterns": [
				{
					"include": "#string-literals"
				},
				{
					"include": "#comments"
				}
			]
		},
		"preprocessor-lang": {
			"begin": "(?i)^[ \t]*(#[ \t]*lang)\\b",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.preprocessor.control-directive.lang.QB64"
				}
			},
			"end": "\n",
			"patterns": [
				{
					"include": "#string-literals"
				},
				{
					"include": "#comments"
				}
			]
		},
		"preprocessor-print": {
			"begin": "(?i)^[ \t]*(#[ \t]*print)\\b",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.preprocessor.control-directive.print.QB64"
				}
			},
			"end": "\n"
		},
		"preprocessor-error": {
			"begin": "(?i)^[ \t]*(#[ \t]*error)\\b",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.preprocessor.control-directive.error.QB64"
				}
			},
			"end": "\n"
		},
		"preprocessor-assert": {
			"begin": "(?i)^[ \t]*(#[ \t]*assert)\\b",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.preprocessor.control-directive.assert.QB64"
				}
			},
			"end": "\n",
			"patterns": [
				{
					"include": "#expressions"
				}
			]
		},
		"preprocessor-line": {
			"begin": "(?i)^[ \t]*(#[ \t]*line)\\b",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.preprocessor.control-directive.line.QB64"
				}
			},
			"end": "\n",
			"patterns": [
				{
					"include": "#literals"
				},
				{
					"include": "#single-quote-comments"
				}
			]
		},
		"options": {
			"name": "meta.option.QB64",
			"begin": "(?i)\\b(option)\\b",
			"beginCaptures": {
				"1": {
					"name": "keyword.other.option.QB64"
				}
			},
			"end": "\n",
			"patterns": [
				{
					"name": "keyword.other.option.QB64",
					"match": "(?i)\\b(?:base|byval|dynamic|escape|explicit|gosub|nogosub|nokeyword|title)\\b"
				},
				{
					"include": "#decimal-integer-literals"
				}
			]
		},
		"expressions": {
			"patterns": [
				{
					"include": "#literals"
				},
				{
					"include": "#operators"
				},
				{
					"include": "#single-quote-comments"
				},
				{
					"include": "#keywords"
				}
			]
		},
		"literals": {
			"patterns": [
				{
					"include": "#numeric-literals"
				},
				{
					"include": "#string-literals"
				},
				{
					"include": "#boolean-literals"
				}
			]
		},
		"numeric-literals": {
			"patterns": [
				{
					"include": "#integer-literals"
				},
				{
					"include": "#floating-point-literals"
				}
			]
		},
		"integer-literals": {
			"patterns": [
				{
					"include": "#decimal-integer-literals"
				},
				{
					"include": "#hexadecimal-integer-literals"
				},
				{
					"include": "#octal-integer-literals"
				},
				{
					"include": "#binary-integer-literals"
				}
			]
		},
		"decimal-integer-literals": {
			"name": "constant.numeric.integer.decimal.QB64",
			"match": "(?i)\\b[0-9]+([%&]|U?L{0,2})?\\b",
			"captures": {
				"1": {
					"name": "storage.type.suffix.integer.QB64"
				}
			}
		},
		"hexadecimal-integer-literals": {
			"name": "constant.numeric.integer.hexadecimal.QB64",
			"match": "(?i)(&h)[0-9A-F]+([%&]|U?L{0,2})?",
			"captures": {
				"1": {
					"name": "keyword.other.integer.hexadecimal.QB64"
				},
				"2": {
					"name": "storage.type.suffix.integer.QB64"
				}
			}
		},
		"octal-integer-literals": {
			"name": "constant.numeric.integer.octal.QB64",
			"match": "(?i)(&o)[0-7]+([%&]|U?L{0,2})?",
			"captures": {
				"1": {
					"name": "keyword.other.integer.octal.QB64"
				},
				"2": {
					"name": "storage.type.suffix.integer.QB64"
				}
			}
		},
		"binary-integer-literals": {
			"name": "contant.numeric.integer.binary.QB64",
			"match": "(?i)(&b)[01]+([%&]|U?L{0,2})?",
			"captures": {
				"1": {
					"name": "keyword.other.integer.binary.QB64"
				},
				"2": {
					"name": "storage.type.suffix.integer.QB64"
				}
			}
		},
		"floating-point-literals": {
			"name": "constant.numeric.floating-point.QB64",
			"match": "(?i)[0-9]*\\.[0-9]+(?:([de])([+\\-]?)[0-9]+)?([!#fd])?",
			"captures": {
				"1": {
					"name": "keyword.other.floating-point.QB64"
				},
				"2": {
					"name": "keyword.operator.floating-point.QB64"
				},
				"3": {
					"name": "storage.type.suffix.floating-point.QB64"
				}
			}
		},
		"string-literals": {
			"patterns": [
				{
					"include": "#escaped-string-literals"
				},
				{
					"include": "#unescaped-string-literals"
				}
			]
		},
		"unescaped-string-literals": {
			"name": "string.quoted.double.unescaped.QB64",
			"begin": "(\\$)?\"",
			"beginCaptures": {
				"1": {
					"name": "keyword.other.string.unescaped.QB64"
				}
			},
			"end": "\""
		},
		"escaped-string-literals": {
			"name": "string.quoted.double.escaped.QB64",
			"begin": "(!)\"",
			"beginCaptures": {
				"1": {
					"name": "keyword.other.string.escaped.QB64"
				}
			},
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.QB64",
					"match": "\\\\(?:[abflnrtv\\\"']|[0-9]{3}|&[hH][0-9a-fA-F]{2}|&[oO][0-7]{3}|&[bB][01]{8}|&[uU][0-9a-fA-F]{4})"
				}
			]
		},
		"interpolated-string-literals": {
			"patterns": [
				{
					"include": "#qbasic-interpolated-string-literals"
				},
				{
					"include": "#visualbasic-interpolated-string-literals"
				}
			]
		},
		"qbasic-interpolated-string-literals": {
			"patterns": [
				{
					"include": "#escaped-qbasic-interpolated-string-literals"
				},
				{
					"include": "#unescaped-qbasic-interpolated-string-literals"
				}
			]
		},
		"unescaped-qbasic-interpolated-string-literals": {
			"name": "string.interpolated.qbasic.unescaped.QB64",
			"begin": "(\\$)?\"",
			"beginCaptures": {
				"1": {
					"name": "keyword.other.string.unescaped.QB64"
				}
			},
			"end": "\"",
			"patterns": [
				{
					"name": "constant.other.string-interpolation.qbasic.QB64",
					"match": "(?:[!\\&#,\\.\\+\\-]|\\^{3,5}|\\*{2}\\$?)"
				}
			]
		},
		"escaped-qbasic-interpolated-string-literals": {
			"name": "string.interpolated.qbasic.unescaped.QB64",
			"begin": "(!)\"",
			"beginCaptures": {
				"1": {
					"name": "keyword.other.string.escaped.QB64"
				}
			},
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.QB64",
					"match": "\\\\(?:[abflnrtv\\\"']|[0-9]{3}|&[hH][0-9a-fA-F]{2}|&[oO][0-7]{3}|&[bB][01]{8}|&[uU][0-9a-fA-F]{4})"
				},
				{
					"name": "constant.other.string-interpolation.qbasic.QB64",
					"match": "(?:[!\\&#,\\.\\+\\-]|\\^{3,5}|\\*{2}\\$?)"
				}
			]
		},
		"visualbasic-interpolated-string-literals": {
			"patterns": [
				{
					"include": "#escaped-visualbasic-interpolated-string-literals"
				},
				{
					"include": "#unescaped-visualbasic-interpolated-string-literals"
				}
			]
		},
		"unescaped-visualbasic-interpolated-string-literals": {
			"name": "string.interpolated.visualbasic.unescaped.QB64",
			"begin": "(\\$)?\"",
			"beginCaptures": {
				"1": {
					"name": "keyword.other.string.unescaped.QB64"
				}
			},
			"end": "\"",
			"patterns": [
				{
					"name": "constant.other.string-interpolation.visualbasic.QB64",
					"match": "(?:[0#%/\\.\\\\:]|[eE][\\+\\-]|\\b(?:d{1,5}|m{1,5}|M{1,2}|y{1,2}|y{4}|h{1,2}|s{1,2}|t{5}|AM/PM|am/pm|A/P|a/p)\\b)"
				}
			]
		},
		"escaped-visualbasic-string-literals": {
			"name": "string.interpolated.qbasic.unescaped.QB64",
			"begin": "(!)\"",
			"beginCaptures": {
				"1": {
					"name": "keyword.other.string.escaped.QB64"
				}
			},
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.QB64",
					"match": "\\\\(?:[abflnrtv\\\"']|[0-9]{3}|&[hH][0-9a-fA-F]{2}|&[oO][0-7]{3}|&[bB][01]{8}|&[uU][0-9a-fA-F]{4})"
				},
				{
					"name": "constant.other.string-interpolation.visualbasic.QB64",
					"match": "(?:[0#%/\\.\\\\:]|[eE][\\+\\-]|\\b(?:d{1,5}|m{1,5}|M{1,2}|y{1,2}|y{4}|h{1,2}|s{1,2}|t{5}|AM/PM|am/pm|A/P|a/p)\\b)"
				}
			]
		},
		"boolean-literals": {
			"name": "constant.language.boolean.QB64",
			"match": "(?i)\\b(true|false)\\b"
		},
		"standard-data-types": {
			"patterns": [
				{
					"include": "#integer-types"
				},
				{
					"include": "#floating-point-types"
				},
				{
					"include": "#string-types"
				},
				{
					"include": "#procedure-types"
				}
			]
		},
		"integer-types": {
			"name": "storage.type.integer.QB64",
			"match": "(?i)\\bu?(?:byte|_byte|short|long|integer|unsigned|integer64|_integer64|_unsigned|offset|_offset)\\b"
		},
		"floating-point-types": {
			"name": "storage.type.floating-point.QB64",
			"match": "(?i)\\b(?:single|double|float|offest)\\b"
		},
		"string-types": {
			"name": "storage.type.string.QB64",
			"match": "(?i)\\b[zw]?string\\b"
		},
		"procedure-types": {
			"name": "storage.type.procedure.QB64",
			"match": "(?i)\\b(?:function|sub)\\b"
		},
		"operators": {
			"name": "keyword.operator.QB64",
			"match": "(?i)(?:[\\+\\-\\*/\\\\^@&\\.,;]|=>?|<>|[<>]=?|->|$|\\b(?:cast|procptr|strptr|varptr|new|delete|mod|shl|shr|not|and|or|eqv|imp|xor|let)\\b)"
		},
		"declarations": {
			"patterns": [
				{
					"include": "#variable-declarations"
				},
				{
					"include": "#procedure-declarations"
				},
				{
					"include": "#type-alias-declarations"
				},
				{
					"include": "#type-declarations"
				},
				{
					"include": "#union-declarations"
				},
				{
					"include": "#enumeration-declarations"
				},
				{
					"include": "#class-declarations"
				},
				{
					"include": "#instance-declarations"
				}
			]
		},
		"variable-declarations": {
			"name": "meta.declaration.variable.QB64",
			"begin": "(?i)\\b(common|const|dim|extern|redim|static)\\b",
			"beginCaptures": {
				"1": {
					"name": "keyword.other.declaration.variable.QB64"
				}
			},
			"end": ":|(?<!_)[ \t]*\n",
			"patterns": [
				{
					"name": "keyword.other.declaration.variable.QB64",
					"match": "(?i)\\b(?:as|preserve)\\b"
