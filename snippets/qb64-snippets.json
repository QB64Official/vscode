{
	"Print": {
		"prefix": "Print",
		"body": [
			"Print $0"
		],
		"description": "Print "
	},
	"#IfElse": {
		"prefix": "ifelse",
		"body": [
			"#if $0 then",
			"",
			"#else",
			"",
			"#end If"
		],
		"description": "#If... "
	},
	"If Else": {
		"prefix": "ifelse",
		"body": [
			"if $0 then",
			"",
			"else",
			"",
			"end if"
		],
		"description": "If...else "
	},
	"If Then": {
		"prefix": "ifthen",
		"body": [
			"if $0 then",
			"",
			"End If"
		],
		"description": "if...then "
	},
	"For Next Loop": {
		"prefix": "for",
		"body": [
			"Dim ${1:index} as Integer",
			"For ${1:index} As ${2:ObjectType} = ${3:lower} To ${4:Upper}",
			"\t$0",
			"Next ${1:index}"
		],
		"description": "For Next Loop"
	},
	"For i...Next i": {
		"prefix": "fori",
		"body": [
			"Dim i as Integer",
			"For i = ${2:Lower} To ${3:Upper}",
			"\t$0",
			"Next i"
		],
		"description": "For i...Next i"
	},
	"For j...Next j": {
		"prefix": "forj",
		"body": [
			"Dim j as Integer",
			"For j = ${2:Lower} To ${3:Upper}",
			"\t$0",
			"Next j"
		],
		"description": "For j...Next j"
	},
	"Function...": {
		"prefix": "func",
		"body": [
			"Function ${1:FunctionName}(${2:ParameterList})",
			"\t$0",
			"End Function"
		],
		"description": "Public Function..."
	},
	"Sub ...": {
		"prefix": "sub",
		"body": [
			"Sub ${1:ProcedureName}(${2:ParameterList})",
			"\t$0",
			"End Sub"
		],
		"description": "Public Sub ..."
	},
	"While ... End While": {
		"prefix": "while",
		"body": [
			"While ${1:Boolean}",
			"\t$0",
			"End While"
		],
		"description": "While ... End While"
	},
	"Type...End Type": {
		"prefix": "type",
		"body": [
			"Type ${1:TypeName}",
			"\t$0",
			"End Type"
		],
		"description": "Public Sub ..."
	}
}